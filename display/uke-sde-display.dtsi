&sde_dsi_active {
	mux {
		pins = "gpio2";
		function = "gpio";
	};
	config {
		pins = "gpio2";
		drive-strength = <8>;   /* 8 mA */
		bias-disable = <0>;   /* no pull */
	};
};

&sde_dsi_suspend {
	mux {
		pins = "gpio2";
		function = "gpio";
	};
	config {
		pins = "gpio2";
		drive-strength = <2>;   /* 2 mA */
		bias-pull-down;         /* PULL DOWN */
	};
};

&L8B {
	regulator-min-microvolt = <1900000>;
	regulator-max-microvolt = <1900000>;
	qcom,init-voltage = <1900000>;
};


&dsi_r66451_amoled_video {
	qcom,panel-supply-entries = <&dsi_panel_pwr_supply_O82>;
	qcom,mdss-dsi-bl-pmic-control-type = "bl_ctrl_dcs";
	qcom,mdss-dsi-bl-min-level = <1>;
	qcom,mdss-dsi-bl-max-level = <4095>;
	qcom,mdss-brightness-max-level = <4095>;
	qcom,mdss-dsi-bl-inverted-dbv;
	qcom,platform-reset-gpio = <&tlmm 2 0>;
};

&sde_dsi {
	qcom,dsi-default-panel = <&dsi_r66451_amoled_video>;
	vddil8b-supply = <&L8B>;
	vsp-supply = <&display_panel_vsp>;
	vsn-supply = <&display_panel_vsn>;
};

&sde_dsi1 {
	status = "disabled";
};

&sde_dp {
	qcom,dp-aux-switch = <&wcd_usbss>;
	qcom,max-lclk-frequency-khz = <540000>;
	status = "ok";
};

&mdss_mdp {
	connectors = <&sde_dsi &smmu_sde_unsec &sde_wb1 &sde_wb2 &sde_rscc &sde_dp>;
};

/* close ssc */
&mdss_dsi_phy0 {
    qcom,dsi-pll-ssc-disable;
};
&mdss_dsi_phy1 {
    qcom,dsi-pll-ssc-disable;
};

&qupv3_se0_i2c {
	status = "ok";
	ktz8866@11 {
		compatible = "ktz,ktz8866";
		status = "ok";
		reg = <0x11>;
		//ktz8866,hwen-gpio = <&tlmm 128 0x00>;
		//ktz8866,enp-gpio = <&tlmm 30 0x00>;
		//ktz8866,enn-gpio = <&tlmm 31 0x00>;
		//ktz8866,panelid-gpio = <&tlmm 10 0x00>;
		//ktz8866,backlight-HBM-enable;
	};
};

&qupv3_se12_i2c {
	status = "ok";
	ktz8866@11 {
		compatible = "ktz,ktz8866";
		status = "ok";
		reg = <0x11>;
	};
};

&qupv3_se4_spi {
	novatek@0 {
		panel = <&dsi_o82_42_02_0b_dualdsi_dsc_lcd_video
		&dsi_o82_36_02_0a_dualdsi_dsc_lcd_video>;
	};
};

&soc {
	thermal_screen: thermal-screen {
		panel = <&dsi_o82_42_02_0b_dualdsi_dsc_lcd_video
		&dsi_o82_36_02_0a_dualdsi_dsc_lcd_video
		&dsi_r66451_amoled_video>;
	};
	charge_screen: charge-screen {
		panel = <&dsi_o82_42_02_0b_dualdsi_dsc_lcd_video
		&dsi_o82_36_02_0a_dualdsi_dsc_lcd_video
		&dsi_r66451_amoled_video>;
	};
	fingerprint_screen: fingerprint-screen {
		panel = <&dsi_o82_42_02_0b_dualdsi_dsc_lcd_video
		&dsi_o82_36_02_0a_dualdsi_dsc_lcd_video
		&dsi_r66451_amoled_video>;
	};
};

&qupv3_se3_i2c {
	nanosic@4c{
		panel = <&dsi_r66451_amoled_video
				&dsi_o82_42_02_0b_dualdsi_dsc_lcd_video
				&dsi_o82_36_02_0a_dualdsi_dsc_lcd_video>;
	};
};