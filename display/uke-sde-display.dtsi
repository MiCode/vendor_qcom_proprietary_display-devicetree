#include "cliffs-sde-display-mtp.dtsi"
#include "dsi-panel-r66451-dsc-fhd-plus-video.dtsi"
#include "dsi-panel-o82-42-02-0b-dualdsi-dsc-lcd-video.dtsi"
#include "dsi-panel-o82-36-02-0a-dualdsi-dsc-lcd-video.dtsi"

&soc {
	dsi_panel_pwr_supply_O82: dsi_panel_pwr_supply_O82 {
		#address-cells = <1>;
		#size-cells = <0>;
		qcom,panel-supply-entry@0 {
			reg = <0>;
			qcom,supply-name = "vddil8b";
			qcom,supply-min-voltage = <1900000>;
			qcom,supply-max-voltage = <1900000>;
			qcom,supply-enable-load = <300000>;
			qcom,supply-disable-load = <0>;
			qcom,supply-post-on-sleep = <2>;
			qcom,supply-post-off-sleep = <2>;
		};

		qcom,panel-supply-entry@1 {
			reg = <1>;
			qcom,supply-name = "vsp";
			qcom,supply-min-voltage = <5800000>;
			qcom,supply-max-voltage = <5800000>;
			qcom,supply-enable-load = <300000>;
			qcom,supply-disable-load = <0>;
			qcom,supply-post-on-sleep = <2>;
			qcom,supply-post-off-sleep = <2>;
		};

		qcom,panel-supply-entry@2 {
			reg = <2>;
			qcom,supply-name = "vsn";
			qcom,supply-min-voltage = <5800000>;
			qcom,supply-max-voltage = <5800000>;
			qcom,supply-enable-load = <300000>;
			qcom,supply-disable-load = <0>;
			qcom,supply-post-on-sleep = <2>;
			qcom,supply-post-off-sleep = <2>;
		};
	};
};

&soc {
	display_panel_vsp: display_gpio_regulator_vsp {
		compatible = "regulator-fixed";
		regulator-name = "display_panel_vsp";
		regulator-min-microvolt = <5800000>;
		regulator-max-microvolt = <5800000>;
		regulator-enable-ramp-delay = <233>;
		gpio = <&tlmm 74 0>;
		enable-active-high;
		regulator-boot-on;
	};

	display_panel_vsn: display_gpio_regulator_vsn {
		compatible = "regulator-fixed";
		regulator-name = "display_panel_vsn";
		regulator-min-microvolt = <5800000>;
		regulator-max-microvolt = <5800000>;
		regulator-enable-ramp-delay = <233>;
		gpio = <&tlmm 75 0>;
		enable-active-high;
		regulator-boot-on;
	};
};


&dsi_o82_42_02_0b_dualdsi_dsc_lcd_video {
	qcom,panel-supply-entries = <&dsi_panel_pwr_supply_O82>;
	qcom,mdss-dsi-bl-pmic-control-type = "bl_ctrl_external";
	qcom,mdss-dsi-bl-min-level = <4>;
	qcom,mdss-dsi-bl-max-level = <4095>;
	qcom,mdss-brightness-max-level = <4095>;
	qcom,mdss-brightness-init-level = <307>;
	//qcom,mdss-dsi-bl-inverted-dbv;
	qcom,platform-reset-gpio = <&tlmm 2  0>;
	mi,panel-id = <0x004F3832 0x5042020B>;
};

&dsi_o82_42_02_0b_dualdsi_dsc_lcd_video {
	qcom,dsi-ctrl-num = <0 1>;
	qcom,dsi-phy-num = <0 1>;
	qcom,dsi-select-clocks = "pll_byte_clk0", "pll_dsi_clk0";
	qcom,mdss-dsi-display-timings {
		timing@120{
			qcom,mdss-dsi-panel-phy-timings = [00 27 0A 0A 1B 19 0A
				0B 0A 02 04 00 20 0F];
			qcom,display-topology = <2 2 2>;
			qcom,default-topology-index = <0>;
		};
		timing@144{
			qcom,mdss-dsi-panel-phy-timings = [00 27 0A 0A 1B 19 0A
				0B 0A 02 04 00 20 0F];
			qcom,display-topology = <2 2 2>;
			qcom,default-topology-index = <0>;
		};
		timing@90{
			qcom,mdss-dsi-panel-phy-timings = [00 27 0A 0A 1B 19 0A
				0B 0A 02 04 00 20 0F];
			qcom,display-topology = <2 2 2>;
			qcom,default-topology-index = <0>;
		};
	};
};

&dsi_o82_36_02_0a_dualdsi_dsc_lcd_video {
	qcom,panel-supply-entries = <&dsi_panel_pwr_supply_O82>;
	qcom,mdss-dsi-bl-pmic-control-type = "bl_ctrl_external";
	qcom,mdss-dsi-bl-min-level = <4>;
	qcom,mdss-dsi-bl-max-level = <4095>;
	qcom,mdss-brightness-max-level = <4095>;
	qcom,mdss-brightness-init-level = <307>;
	//qcom,mdss-dsi-bl-inverted-dbv;
	qcom,platform-reset-gpio = <&tlmm 2  0>;
	mi,panel-id = <0x004F3832 0x5036020A>;
};

&dsi_o82_36_02_0a_dualdsi_dsc_lcd_video {
	qcom,dsi-ctrl-num = <0 1>;
	qcom,dsi-phy-num = <0 1>;
	qcom,dsi-select-clocks = "pll_byte_clk0", "pll_dsi_clk0";
	qcom,mdss-dsi-display-timings {
		timing@120 {
			qcom,mdss-dsi-panel-phy-timings = [00 27 0A 0A 1B 19 0A
				0B 0A 02 04 00 20 0F];
			qcom,display-topology = <2 2 2>;
			qcom,default-topology-index = <0>;
		};
		timing@144{
			qcom,mdss-dsi-panel-phy-timings = [00 27 0A 0A 1B 19 0A
				0B 0A 02 04 00 20 0F];
			qcom,display-topology = <2 2 2>;
			qcom,default-topology-index = <0>;
		};
		timing@90 {
			qcom,mdss-dsi-panel-phy-timings = [00 27 0A 0A 1B 19 0A
				0B 0A 02 04 00 20 0F];
			qcom,display-topology = <2 2 2>;
			qcom,default-topology-index = <0>;
		};
	};
